{
  "name": "@maninacan/react-uikit-styleguide-template",
  "version": "0.0.1",
  "description": "As the name infers it is a React UIkit style-guide.",
  "author": "Maninacan",
  "license": "MIT",
  "repository": "Maninacan/react-uikit-styleguide-template",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "fonts:choose": "npx fontello-cli open --config fontello.config.json",
    "fonts:install": "npm run storybook:fontsinject && npx fontello-cli --config fontello.config.json --css src/fontello/css/ --font src/fontello/font/ install",
    "storybook:fontsinject": "cd ./src/components/Icons/ && rm -rf ./Icons.js && cp ./IconsTemplate.js ./Icons.js && node ./iconInjectionScript.js && prettier --write \"./Icons.js\"",
    "storybook": "npm run storybook:fontsinject && start-storybook -p 9090 -c .storybook -s dist",
    "storybook:build": "npm run storybook:fontsinject && build-storybook -c .storybook -o .storybookbuild -s dist",
    "css:buildtheme": "node-sass-chokidar src/{{cookiecutter.project_name}}-theme/main.scss dist/css/{{cookiecutter.project_name}}-theme.css",
    "css:watchtheme": "node-sass-chokidar src/{{cookiecutter.project_name}}-theme/main.scss dist/css/{{cookiecutter.project_name}}-theme.css --watch",
    "css:build": "npm run css:buildtheme",
    "css:watch": "npm run css:watchtheme",
    "prettier": "prettier --ignore-path \"./.prettierignore\" \"**/*.+(js|json|html|scss|md)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run prettier -- --list-different",
    "module:build": "npm run copy && rollup -c",
    "module:watch": "npm run copy && rollup -c -w",
    "test": "jest",
    "lint": "eslint \"./src/**/*.js\"",
    "lint:fix": "eslint --fix \"./src/**/*.js\"",
    "clean": "npx del-cli dist/**",
    "copy": "copyfiles -u 1 src/images/**/* dist/assets/ && copyfiles -u 1 src/fontello/**/* dist/assets/",
    "build": "npm run clean && npm run css:build && npm run module:build",
    "start": "concurrently -k -n CSS,BASE_STYLES,STORYBOOK -c blue,green,yellow npm:css:watch npm:module:watch npm:storybook",
    "prepare": "yarn run build",
    "predeploy": "yarn run build"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-info": "^4.1.11",
    "@storybook/addon-knobs": "^4.1.11",
    "@storybook/addon-links": "^4.1.11",
    "@storybook/addon-notes": "^4.1.11",
    "@storybook/addon-options": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "@svgr/rollup": "^4.1.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "chalk-cli": "^4.1.0",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.1.4",
    "del-cli": "^1.1.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^4.0.0",
    "gh-pages": "^2.0.1",
    "node-sass-chokidar": "^1.3.4",
    "prettier": "^1.16.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "^2.1.3",
    "replace-in-file": "^3.4.3",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.1",
    "rollup-plugin-url": "^2.1.0",
    "storybook-addon-smart-knobs": "^4.1.0",
    "storybook-readme": "^4.0.5",
    "uikit": "^3.0.2"
  },
  "files": [
    "src/{{cookiecutter.project_name}}-theme/**/*",
    "dist/**/*"
  ],
  "dependencies": {
    "react-clipboard.js": "^2.0.2",
    "uikit": "^3.0.2"
  }
}
